{
    package parser

    import (
        "github.com/brimdata/super/compiler/ast"
    )
}

start
  = __ scope:Scope EndQuery { return []any{scope}, nil }
  / __ seq:Seq EndQuery { return seq, nil }

EndQuery = __ ";"? __ EOF

Scope
  = decls:Decl+ body:Seq {
      return &ast.Scope{
        Kind: "Scope",
        Decls: sliceOf[ast.Decl](decls),
        Body: sliceOf[ast.Op](body),
        Loc: loc(c),
      }, nil
    }

Seq
  = first:LeanOp rest:SeqTail* {
      return prepend(first, rest), nil
    }

SeqTail = __ Pipe __ o:LeanOp { return o, nil }

Decl
  = v:(ConstDecl / FuncDecl / OpDecl / TypeDecl) _ { return v, nil }

ConstDecl
  = CONST _ name:Identifier __ "=" __ expr:Expr {
      return &ast.ConstDecl{
          Kind: "ConstDecl",
          Name: name.(*ast.ID),
          Expr: expr.(ast.Expr),
          Loc: loc(c),
      }, nil
    }

FuncDecl
  = FUNC _ name:Identifier __ "(" __ params:Identifiers? __ ")" __ ":" __ "(" __ expr:Expr __ ")" {
      return &ast.FuncDecl{
        Kind: "FuncDecl",
        Name: name.(*ast.ID),
        Params: sliceOf[*ast.ID](params),
        Expr: expr.(ast.Expr),
        Loc: loc(c),
      }, nil
    }

OpDecl
  = OP _ name:Identifier __ "(" __ params:Identifiers? __ ")" __ ":" __ "(" __ body:OpDeclBody __ ")" {
      return &ast.OpDecl{
        Kind: "OpDecl",
        Name: name.(*ast.ID),
        Params: sliceOf[*ast.ID](params),
        Body: sliceOf[ast.Op](body),
        Loc: loc(c),
      }, nil
    }

OpDeclBody
  = scope:Scope { return []any{scope}, nil }
  / seq:Seq { return seq, nil }

TypeDecl
  = TYPE _ name:Identifier __ "=" __ typ:Type {
      return &ast.TypeDecl{
        Kind: "TypeDecl",
        Name: name.(*ast.ID),
        Type: typ.(ast.Type),
        Loc: loc(c),
      }, nil
    }

// A LeanOp is a pipeline operator along with various forms of 
// "lean-forward" shortcuts: a scope with other LeanOps, a shortcut assignment 
// for "put", a shortcut "aggregate", or an expression that is either
// implied yield (non-boolean) or implied where (boolean).
LeanOp
  = Operator
  / "(" __ scope:Scope __ ")" { return scope, nil }
  / a:OpAssignment &EndOfOp { return a, nil }
  / !(Function EndOfOp) a:Aggregation &EndOfOp { return a, nil }
  / !((Identifier / Literal) __ (Pipe/EOF)) e:Expr {
      return &ast.OpExpr{Kind:"OpExpr",Expr:e.(ast.Expr),Loc:loc(c)}, nil
    }

EndOfOp = __ (Pipe / SearchKeywordGuard / "=>" / ")" / ";" / EOF)
Pipe = "|>" / "|" 

ExprGuard = __ ((!"=>" Comparator) / AdditiveOperator / MultiplicativeOperator / ":" / "(" / "[" / "~")

Comparator
  = ("==" / "=" / "!=" / "<>" / IN / LIKE / "<=" / "<" / ">=" / ">") { return string(c.text), nil }
  / NOT _ LIKE { return "not like", nil }

SearchBoolean
  = first:SearchAnd rest:SearchOrTerm* {
      return makeBinaryExprChain(first, rest, c), nil
    }

SearchOrTerm = _ OR _ t:SearchAnd { return []any{"or", t}, nil }

SearchAnd
  = first:SearchFactor
    rest:((_ AND)? _ !(OR / SearchKeywordGuard) expr:SearchFactor { return []any{"and", expr}, nil })* {
      return makeBinaryExprChain(first, rest, c), nil
    }

SearchKeywordGuard
  = FromSource __ "=>" __
  / Case __

SearchFactor
  = (NOT _ / "!" __) e:SearchFactor {
      return &ast.UnaryExpr{
          Kind: "UnaryExpr",
          Op: "!",
          Operand: e.(ast.Expr),
          Loc: loc(c),
      }, nil
    }
  / "(" __ expr:SearchBoolean __ ")" { return expr, nil }
  / SearchExpr

SearchExpr
  = Regexp
  / Glob
  / v:SearchValue (!ExprGuard / &(_ Glob)) {
      return &ast.Term{
        Kind: "Term",
        Text: string(c.text),
        Value: v.(ast.Any),
        Loc: loc(c),
      }, nil
    }
  / "*" !ExprGuard {
      return &ast.Primitive{Kind:"Primitive",Type:"bool",Text:"true",Loc:loc(c)}, nil
    }
  / SearchPredicate

SearchPredicate
  = lhs:AdditiveExpr __ op:Comparator __ rhs:AdditiveExpr {
      return &ast.BinaryExpr{
        Kind: "BinaryExpr",
        Op: op.(string),
        LHS: lhs.(ast.Expr),
        RHS: rhs.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / f:Function { return f, nil }

SearchValue
  = Literal
  / !RegexpPattern v:KeyWord {
      return newPrimitive(c, "string", v.(string)), nil
    }

Glob
  = pattern:GlobPattern {
      return &ast.Glob{Kind: "Glob", Pattern: pattern.(string), Loc: loc(c)}, nil
    }

Regexp
  = pattern:RegexpPattern {
      return &ast.Regexp{Kind:"Regexp", Pattern:pattern.(string), Loc: loc(c)}, nil
    }

/// === Aggregations ===

Aggregation
  = Aggregate? keys:AggregateKeys limit:LimitArg {
      return &ast.Aggregate{
        Kind: "Aggregate",
        Keys: sliceOf[ast.Assignment](keys),
        Limit: limit.(int),
        Loc: loc(c),
      }, nil
    }
  / Aggregate? aggs:AggAssignments keys:(_ AggregateKeys)? limit:LimitArg {
      s := &ast.Aggregate {
        Kind: "Aggregate",
        Aggs: sliceOf[ast.Assignment](aggs),
        Limit: limit.(int),
        Loc: loc(c),
      }
      if keys != nil {
        s.Keys = sliceOf[ast.Assignment](keys.([]any)[1])
      }
      return s, nil
    }

Aggregate = (AGGREGATE / SUMMARIZE) _

AggregateKeys
  = (GROUP _)? BY _ columns:FlexAssignments { return columns, nil }

LimitArg
  = _ WITH _ "-limit" _ limit:UInt { return limit, nil }
  / "" { return 0, nil }

// A FlexAssignment is like an Assignment but it can optionally omit the lhs,
// in which case the semantic pass will infer a name from the rhs, e.g., for
// an expression like "count() by foo", the rhs is Field "foo" and the lhs is nil.
FlexAssignment
  = Assignment
  / expr:Expr { return ast.Assignment{Kind:"Assignment",RHS:expr.(ast.Expr),Loc:loc(c)}, nil }

FlexAssignments
  = first:FlexAssignment rest:(__ "," __ expr:FlexAssignment { return expr, nil })* {
      return prepend(first, rest), nil
    }

AggAssignment
  = lval:Lval __ ":=" __ agg:Agg {
      return ast.Assignment{Kind:"Assignment",LHS:lval.(ast.Expr),RHS:agg.(ast.Expr),Loc:loc(c)}, nil
    }
  / agg:Agg {
      return ast.Assignment{Kind:"Assignment",RHS:agg.(ast.Expr),Loc:loc(c)}, nil
    }

Agg
  = !FuncGuard aggDistinct:AggDistinct !(__ ".") where:WhereClause? {
      agg := aggDistinct.(*ast.Agg)
      agg.Loc = loc(c)
      if where != nil {
        agg.Where = where.(ast.Expr)
      }
      return agg, nil
    }
  / !FuncGuard name:AggName __ "(" __ expr:(OverExpr / Expr)? __ ")" !(__ ".") where:WhereClause? {
      agg := &ast.Agg{
          Kind: "Agg",
          Name: name.(string),
          Loc: loc(c),
      }
      if expr != nil {
        agg.Expr = expr.(ast.Expr)
      }
      if where != nil {
        agg.Where = where.(ast.Expr)
      }
      return agg, nil
    }
  / cs:CountStar {
      return &ast.Agg{
          Kind: "Agg",
          Name: "count",
          Loc: cs.(*ast.Call).Loc,
      }, nil
    }

AggDistinct
  = !FuncGuard name:AggName __ "(" __ DISTINCT _ expr:(OverExpr / Expr) __ ")" {
      return &ast.Agg{
          Kind: "Agg",
          Name: name.(string),
          Distinct: true,
          Expr: expr.(ast.Expr),
          Loc: loc(c),
      }, nil
    }

AggName
  = IdentifierName
  / AND
  / OR

WhereClause = _ WHERE _ expr:LogicalOrExpr { return expr, nil }

AggAssignments
  = first:AggAssignment rest:(__ "," __ AggAssignment)* {
      result := []any{first}
      for _, r := range rest.([]any) {
          result = append(result, r.([]any)[3])
      }
      return result, nil
    }

CountStar = COUNT __ "(" __ "*" __ ")" {
    return &ast.Call{
		Kind: "Call",
		Name: &ast.ID{Kind:"ID", Name:"count", Loc: ast.NewLoc(c.pos.offset, c.pos.offset+5)},
        Loc:  loc(c),
    }, nil
  }

/// === Operators ===

Operator
  = op:SelectOp &EndOfOp { return op, nil }
  / ForkOp
  / SwitchOp 
  / FromForkOp
  / SearchOp
  / AssertOp
  / SortOp
  / TopOp
  / CutOp
  / DistinctOp
  / DropOp
  / HeadOp
  / TailOp
  / SkipOp
  / WhereOp
  / UniqOp
  / PutOp
  / RenameOp
  / FuseOp
  / ShapeOp
  / JoinOp
  / SampleOp
  / FromOp
  / PassOp
  / ExplodeOp
  / MergeOp
  / OverOp
  / YieldOp
  / LoadOp
  / OutputOp
  / DebugOp

PipeKeyword
  = SELECT / FORK / SWITCH / FROM / SEARCH / ASSERT / SORT / TOP/ CUT / DROP
  / HEAD / TAIL / WHERE / UNIQ / PUT / RENAME / FUSE / SHAPE / JOIN / SAMPLE
  / FROM / PASS / EXPLODE / MERGE / OVER / YIELD / LOAD / OUTPUT / DEBUG
  / AGGREGATE / SUMMARIZE

 ForkOp =
   FORK __ "(" paths:Path+ __ ")" {
      var seqs []ast.Seq
      for _, p := range paths.([]any) {
        seqs = append(seqs, sliceOf[ast.Op](p))
      }
      return &ast.Parallel{
          Kind: "Parallel",
          Paths: seqs,
          Loc: loc(c),
      }, nil
    }

Path = __ "=>" __ seq:Seq { return seq, nil }

SwitchOp
  = SWITCH _ expr:Expr _ "(" cases:SwitchPath+ __ ")" {
      return &ast.Switch{
          Kind: "Switch",
          Expr: expr.(ast.Expr),
          Cases: sliceOf[ast.Case](cases),
          Loc: loc(c),
      }, nil
    }
  / SWITCH __ "(" cases:SwitchPath+ __ ")" {
      return &ast.Switch{
          Kind: "Switch",
          Cases: sliceOf[ast.Case](cases),
          Loc: loc(c),
      }, nil
    }

SwitchPath
  = __ expr:Case path:Path {
      l := ast.Case{Path: sliceOf[ast.Op](path)}
      if expr != nil {
        l.Expr = expr.(ast.Expr)
      }
      return l, nil
    }

Case
  = CASE _ expr:Expr { return expr, nil }
  / DEFAULT { return nil, nil }

FromForkOp
  = FROM __ "(" trunks:FromPath+ __ ")" {
      return &ast.Parallel{
          Kind: "Parallel",
          Paths: sliceOf[ast.Seq](trunks),
          Loc: loc(c),
      }, nil
    }

FromPath
  = __ source:FromSource seq:(__ "=>" __ s:Seq { return s, nil })? {
      ops := []any{source}
      if seq != nil {
        ops = append(ops, seq.([]any)...)
      }
      return ast.Seq(sliceOf[ast.Op](ops)), nil
    }

FromSource
  = FromKeyWord _ elem:FromElem {
        return &ast.From{
            Kind: "From",
            Elems: []*ast.FromElem{elem.(*ast.FromElem)},
            Loc: loc(c),
        }, nil
    }
  / PassOp

SearchOp
  = (SEARCH _ / "?" __) expr:SearchBoolean {
      return &ast.Search{Kind:"Search",Expr:expr.(ast.Expr),Loc:loc(c)}, nil
    }

AssertOp
  = ASSERT _ expr:(e:Expr { return []any{e, string(c.text)}, nil }) {
      return &ast.Assert{
          Kind: "Assert",
          Expr: expr.([]any)[0].(ast.Expr),
          Text: expr.([]any)[1].(string),
          Loc: loc(c),
      }, nil
    }

SortOp
  = (SORT / ORDER _ BY) &EOKW args:SortArgs exprs:(__ e:OrderByList { return e, nil })? {
      o := &ast.Sort{
        Kind: "Sort",
        Loc: loc(c),
      }
      if exprs != nil {
        o.Exprs = exprs.([]ast.SortExpr)
      }
      argm := args.(map[string]any)
      if _, ok := argm["r"]; ok {
        o.Reverse = true
      }
      return o, nil
    }

SortArgs = args:(_ a:SortArg { return a, nil })* { return makeArgMap(args) }

SortArg
  = "-r" { return map[string]any{"name": "r", "value": nil}, nil }

TopOp
  = TOP &EOKW args:SortArgs limit:(_ e:Expr { return e, nil })? exprs:(_ e:OrderByList { return e, nil })? {
      op := &ast.Top{
        Kind: "Top",
        Loc: loc(c),
      }
      if limit != nil {
        op.Limit = limit.(ast.Expr)
      }
      if exprs != nil {
        op.Exprs = exprs.([]ast.SortExpr)
      }
      argm := args.(map[string]any)
      if _, ok := argm["r"]; ok {
        op.Reverse = true
      }
      return op, nil
    }

CutOp
  = CUT _ args:FlexAssignments {
      return &ast.Cut{
        Kind: "Cut",
        Args: sliceOf[ast.Assignment](args),
        Loc: loc(c),
      }, nil
    }

DistinctOp
  = DISTINCT _ e:Expr {
      return &ast.Distinct{
          Kind: "Distinct",
          Expr: e.(ast.Expr),
          Loc: loc(c),
      }, nil
    }

DropOp
  = DROP _ args:Lvals {
      return &ast.Drop{
          Kind: "Drop",
          Args: sliceOf[ast.Expr](args),
          Loc: loc(c),
      }, nil
    }

HeadOp
  = (HEAD / LIMIT) _ !EndOfOp count:Expr {
      return &ast.Head{
        Kind: "Head",
        Count: count.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / HEAD !(__ "(") &EOKW {
      return &ast.Head{
        Kind: "Head",
        Loc: loc(c),
      }, nil
    }

TailOp
  = TAIL _ !EndOfOp count:Expr {
      return &ast.Tail{
        Kind: "Tail",
        Count: count.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / TAIL !(__ "(") &EOKW {
      return &ast.Tail{
        Kind: "Tail",
        Loc: loc(c),
      }, nil
    }

SkipOp
  = SKIP _ count:Expr {
      return &ast.Skip{
        Kind: "Skip",
        Count: count.(ast.Expr),
        Loc: loc(c),
      }, nil
    }

WhereOp
  = WHERE _ expr:Expr {
      return &ast.Where{
        Kind: "Where",
        Expr: expr.(ast.Expr),
        Loc: loc(c),
      }, nil
    }

UniqOp
  = UNIQ _ "-c" {
      return &ast.Uniq{Kind: "Uniq", Cflag: true, Loc: loc(c)}, nil
    }
  / UNIQ !(__ "(") &EOKW {
      return &ast.Uniq{Kind: "Uniq", Loc: loc(c)}, nil
    }

PutOp
  = PUT _ args:Assignments {
      return &ast.Put{
          Kind: "Put",
          Args: sliceOf[ast.Assignment](args),
          Loc: loc(c),
      }, nil
    }

RenameOp
  = RENAME _ first:Assignment rest:(__ "," __ cl:Assignment { return cl, nil })* {
      args := append([]any{first}, rest.([]any)...)
      return &ast.Rename{
        Kind: "Rename",
        Args: sliceOf[ast.Assignment](args),
        Loc: loc(c),
      }, nil
    }

// The paren guard is to allow parsing fuse() as an aggregator. This
// is temporary pending finalizing the names
// (https://github.com/brimdata/super/issues/2114) for the fuse op and
// aggregator, at which point we may no longer need this.
FuseOp
  = FUSE !(__ "(") &EOKW {
      return &ast.Fuse{Kind: "Fuse", Loc: loc(c)}, nil
    }

ShapeOp
  = SHAPE !(__ "(") &EOKW {
      return &ast.Shape{Kind: "Shape", Loc: loc(c)}, nil
    }

JoinOp
  = style:JoinStyle JOIN rightInput:JoinRightInput e:JoinExpr optArgs:(_ FlexAssignments)? {
      o := &ast.Join{
        Kind: "Join",
        Style: style.(string),
        Cond: e.(ast.JoinExpr),
        Loc: loc(c),
      }
      if rightInput != nil {
        o.RightInput = sliceOf[ast.Op](rightInput)
      }
      if optArgs != nil {
        o.Args = sliceOf[ast.Assignment](optArgs.([]any)[1])
      }
      return o, nil
    }

JoinStyle
  = ANTI _  { return "anti", nil }
  / INNER _ { return "inner", nil }
  / LEFT  _ { return "left", nil }
  / RIGHT _ { return "right", nil }
  / ""      { return "", nil }

JoinRightInput
  = __ "(" __ s:Seq __ ")" { return s, nil }
  / "" { return nil, nil }

JoinKey
  = Lval
  / "(" expr:Expr ")" { return expr, nil }

SampleOp
  = SAMPLE &EOKW expr:(_ e:Lval { return e, nil })? {
      o := &ast.Sample{Kind: "Sample", Loc: loc(c)}
      if expr != nil {
        o.Expr = expr.(ast.Expr)
      }
      return o, nil
    }

// OpAssignment is a top level operator that contains a list of assignment
// expressions. It is in an ambiguous state and could either be:
//   1) An aggregate operator.
//   2) A put operator.
//  The semantic pass is responsible for converting it to one of the two.
OpAssignment
  = a:Assignments {
      return &ast.OpAssignment{
          Kind: "OpAssignment",
          Assignments: sliceOf[ast.Assignment](a),
          Loc: loc(c),
      }, nil
    }

LoadOp
  = LOAD _ pool:Name branch:PoolBranch? author:AuthorArg? message:MessageArg? meta:MetaArg? {
      return &ast.Load{
        Kind: "Load",
        Pool: nullableName(pool),
        Branch: nullableName(branch),
        Author: nullableName(author),
        Message: nullableName(message),
        Meta: nullableName(meta),
        Loc: loc(c),
      }, nil
    }

AuthorArg
  = _ AUTHOR _ n:Name { return n, nil }

MessageArg
  = _ MESSAGE _ n:Name { return n, nil }

MetaArg
  = _ META _ n:Name { return n, nil }

PoolBranch
  = "@" branch:Name { return branch, nil }

OutputOp
  = OUTPUT _ name:Identifier {
      return &ast.Output{
        Kind: "Output",
        Name: name.(*ast.ID),
        Loc: loc(c),
      }, nil
    }

DebugOp
  = DEBUG &EOKW expr:(_ e:Expr { return e, nil })? {
      d := &ast.Debug{
        Kind: "Debug",
        Loc: loc(c),
      }
      if expr != nil {
        d.Expr = expr.(ast.Expr)
      }
      return d, nil
    }

FromOp
  = FromKeyWord _ elems:FromElems {
      return &ast.From{
        Kind: "From",
        Elems: sliceOf[*ast.FromElem](elems),
        Loc: loc(c),
      }, nil
  }

FromKeyWord
  = FROM
  / DeprecatedFroms

DeprecatedFroms = "get" / "file" / "pool"

FromElems
  = first:FromElem rest:( __ "," __ elem:FromElem { return elem, nil} )* {
      return prepend(first, rest), nil
    }
    
FromElem
  = entity:FromEntity args:FromArgs o:OptOrdinality alias:OptAlias {
      elem := &ast.FromElem{
        Kind: "FromElem",
        Entity: entity.(ast.FromEntity),
        Loc: loc(c),
      }
      if args != nil {
        elem.Args = args.(ast.FromArgs)
      }
      if o != nil {
        elem.Ordinality = o.(*ast.Ordinality)
      }      
      if alias != nil {
        elem.Alias = alias.(*ast.TableAlias)
      }
      return elem, nil
    }

FromEntity
  = url:UnquotedURL {
      return &ast.Name{
        Kind: "Name",
        Text: url.(string),
        Loc: loc(c),
      }, nil
    }
  / Regexp
  / Glob
  / "*" !ExprGuard { return &ast.Glob{Kind: "Glob", Pattern: "*", Loc: loc(c)}, nil }
  / EVAL __ "(" __ e:Expr __ ")" {
      return &ast.ExprEntity{
        Kind: "ExprEntity",
        Expr: e.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / meta:PoolMeta {
      return &ast.LakeMeta{
        Kind: "LakeMeta",
        Meta: nullableName(meta),
        Loc: loc(c),
      }, nil
    }
  / join:JoinOperation !AliasClause {return join, nil}
  / "(" __ join:JoinOperation __ ")" { return join, nil }
  / "(" __ s:SQLPipe __ ")" {
      return s, nil
    }
   / name:Name    { return name, nil }
  // XXX UNNEST, PIVOT, UNPIVOT, type projection, left correlation?

FromArgs
  = commit:PoolCommit meta:PoolMeta? tap:TapArg {
      return &ast.PoolArgs{
        Kind: "PoolArgs",
        Commit: nullableName(commit),
        Meta: nullableName(meta),
        Tap: tap.(bool),
        Loc: loc(c),
      }, nil
    }
  / meta:PoolMeta tap:TapArg {
      return &ast.PoolArgs{
        Kind: "PoolArgs",
        Meta: nullableName(meta),
        Tap: tap.(bool),
        Loc: loc(c),
      }, nil
    }
  / format:FormatArg !(_ (METHOD / HEADERS / BODY)) {
      return &ast.FormatArg{
        Kind: "FormatArg",
        Format: nullableName(format),
        Loc: loc(c),
      }, nil
  }
  / format:FormatArg? method:MethodArg? headers:HeadersArg? body:BodyArg? {
      if format == nil && method == nil && headers == nil && body == nil {
        return nil, nil
      }
      h := &ast.HTTPArgs{
        Kind: "HTTPArgs",
        Format: nullableName(format),
        Method: nullableName(method),
        Body: nullableName(body),
        Loc: loc(c),
      }
      if headers != nil {
        h.Headers = headers.(*ast.RecordExpr)
      }
      return h, nil
    }

FormatArg = _ FORMAT _ n:Name { return n, nil }

MethodArg = _ METHOD _ n:Name { return n, nil }

HeadersArg = _ HEADERS _ v:Record { return v, nil }

BodyArg = _ BODY _ n:Name { return n, nil }

UnquotedURL = ("http://" / "https://") URLChar+ { return string(c.text), nil }

URLChar = [0-9a-zA-Z!@$%&_=,./?:[\]~+-]

//XXX this should be a timestamp
PoolAt
  = _ AT _ id:KSUID { return id, nil }

//XXX this should allow 0x bytes format
KSUID = ([0-9a-zA-Z])+ { return string(c.text), nil }

PoolCommit
  = "@" n:Name { return n, nil }

PoolMeta
  = ":" n:Name { return n, nil }

TapArg
  = _ TAP { return true, nil }
  / "" { return false, nil }

PassOp
  = PASS !(__ "(") &EOKW {
      return &ast.Pass{Kind: "Pass", Loc: loc(c)}, nil
    }

// Currently we support only "explode by type" but this will be generalized
// to explode arbitrary arrays, records, etc.
ExplodeOp
  = EXPLODE _ args:Exprs typ:TypeArg as:AsArg? {
      o := &ast.Explode{
        Kind: "Explode",
        Args: sliceOf[ast.Expr](args),
        Type: typ.(ast.Type),
        Loc: loc(c),
      }
      if as != nil {
        o.As = as.(ast.Expr)
      }
      return o, nil
    }

MergeOp
  = MERGE _ exprs:OrderByList {
      return &ast.Merge{
        Kind: "Merge",
        Exprs: exprs.([]ast.SortExpr),
        Loc: loc(c),
      }, nil
    }

OverOp
  = (OVER / UNNEST) _ exprs:Exprs locals:Locals? body:Lateral? {
      o := &ast.Over{
        Kind: "Over",
        Exprs: sliceOf[ast.Expr](exprs),
        Locals: sliceOf[ast.Def](locals),
        Loc: loc(c),
      }
      if body != nil {
        o.Body = sliceOf[ast.Op](body.([]any)[0])
      }
      return o, nil
    }

//XXX these locators in the any slice need to go

Lateral
  = __ "=>" __ "(" __ scope:Scope __ ")" {
      return []any{[]any{scope.(*ast.Scope)}, loc(c).End()}, nil
    }
  / __ "=>" __ "(" __ seq:Seq __ ")" {
      return []any{seq, loc(c).End()}, nil
    }

Locals
  = _ WITH _ first:LocalsAssignment rest:(__ "," __ a:LocalsAssignment { return a, nil })* {
      return prepend(first, rest), nil
    }

LocalsAssignment
  = name:Identifier opt:(__ "=" __ Expr)? {
      def := ast.Def{
        Name: name.(*ast.ID),
        Expr: name.(*ast.ID),
        Loc: loc(c),
      }
      if opt != nil {
        def.Expr = opt.([]any)[3].(ast.Expr)
      }
      return def, nil
    }

YieldOp
  = YIELD _ exprs:Exprs {
      return &ast.Yield{
          Kind: "Yield",
          Exprs: sliceOf[ast.Expr](exprs),
          Loc: loc(c),
      }, nil
    }

TypeArg
  = _ BY _ typ:Type { return typ, nil }

AsArg
  = _ AS _ lhs:Lval { return lhs, nil }

/// === Expressions ===

Lval = DerefExpr

Lvals
  = first:Lval rest:(__ "," __ lval:Lval { return lval, nil })* {
      return prepend(first, rest), nil
    }

Assignments
  = first:Assignment rest:(__ "," __ a:Assignment { return a, nil })* {
    return prepend(first, rest), nil
  }

Assignment
  = lhs:Lval __ ":=" __ rhs:Expr {
      return ast.Assignment{
          Kind: "Assignment",
          LHS: lhs.(ast.Expr),
          RHS: rhs.(ast.Expr),
          Loc: loc(c),
      }, nil
    }

Expr = ConditionalExpr

ConditionalExpr
  = cond:LogicalOrExpr opt:(__ "?" __ Expr __ ":" __ Expr)? {
      if opt != nil {
        t, e := opt.([]any)[3], opt.([]any)[7]
        return &ast.Conditional{
            Kind: "Conditional",
            Cond: cond.(ast.Expr),
            Then: t.(ast.Expr),
            Else: e.(ast.Expr),
            Loc: loc(c),
        }, nil
      }
      return cond, nil
    }

LogicalOrExpr
  = first:LogicalAndExpr
    rest:(__ op:OR __ expr:LogicalAndExpr { return []any{op, expr}, nil })* {
        return makeBinaryExprChain(first, rest, c), nil
    }

LogicalAndExpr
  = first:NotExpr
    rest:(__ op:AND __ expr:NotExpr { return []any{op, expr}, nil })* {
        return makeBinaryExprChain(first, rest, c), nil
    }

NotExpr
  = (NOT _ / "!" __) e:NotExpr {
      return &ast.UnaryExpr{
          Kind: "UnaryExpr",
          Op: "!",
          Operand: e.(ast.Expr),
          Loc: loc(c),
      }, nil
    }
  / BetweenExpr
  
BetweenExpr
  = expr:ComparisonExpr _ not:(NOT _)? BETWEEN _ lower:BetweenExpr _ AND _ upper:BetweenExpr {
      return &ast.Between{
        Kind: "Between",
        Not: not != nil,
        Expr: expr.(ast.Expr),
        Lower: lower.(ast.Expr),
        Upper: upper.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / ComparisonExpr

ComparisonExpr
  = expr:AdditiveExpr _ IS not:(_ NOT)? _ NULL {
      return &ast.IsNullExpr{
        Kind: "IsNullExpr",
        Expr: expr.(ast.Expr),
        Not: not != nil,
        Loc: loc(c),
      }, nil
    }   
  / lhs:AdditiveExpr opAndRHS:(__ Comparator __ AdditiveExpr / __ ("~" { return string(c.text), nil }) __ Regexp)? {
      if opAndRHS == nil {
        return lhs, nil
      }
      return &ast.BinaryExpr{
        Kind: "BinaryExpr",
        Op: opAndRHS.([]any)[1].(string),
        LHS: lhs.(ast.Expr),
        RHS: opAndRHS.([]any)[3].(ast.Expr),
        Loc: loc(c),
      }, nil
    }
 
AdditiveExpr
  = first:MultiplicativeExpr
    rest:(__ op:AdditiveOperator __ expr:MultiplicativeExpr { return []any{op, expr}, nil })* {
        return makeBinaryExprChain(first, rest, c), nil
    }

AdditiveOperator = ("+" / "-") { return string(c.text), nil }

MultiplicativeExpr
  = first:ConcatExpr
    rest:(__ op:MultiplicativeOperator __ expr:ConcatExpr { return []any{op, expr}, nil })* {
        return makeBinaryExprChain(first, rest, c), nil
    }

MultiplicativeOperator = ("*" / "/" / "%") { return string(c.text), nil }

ConcatExpr 
  = first:UnaryPlusOrMinus
    rest:(__ "||" __ expr:UnaryPlusOrMinus { return []any{"||", expr}, nil })* {
        return makeBinaryExprChain(first, rest, c), nil
    }

UnaryPlusOrMinus
  = !Literal op:PlusOrMinusOp __ e:UnaryPlusOrMinus
 {
      return &ast.UnaryExpr{
          Kind: "UnaryExpr",
          Op: op.(string),
          Operand: e.(ast.Expr),
          Loc: loc(c),
      }, nil
    }
  / DerefExpr

PlusOrMinusOp = ("+" / "-") { return string(c.text), nil }

DerefExpr
  = expr:DerefExpr "[" __ from:AdditiveExpr __ ":" __ to:AdditiveExpr? __ "]" {
      e := &ast.SliceExpr{
        Kind: "SliceExpr",
        Expr: expr.(ast.Expr),
        From: from.(ast.Expr),
        Loc: loc(c),
      }
      if to != nil {
        e.To = to.(ast.Expr)
      }
      return e, nil
    }
  / expr:DerefExpr "[" __ ":" __ to:AdditiveExpr "]" {
      return &ast.SliceExpr{
        Kind: "SliceExpr",
        Expr: expr.(ast.Expr),
        To: to.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / expr:DerefExpr "[" index:Expr "]" {
      return &ast.IndexExpr{
        Kind: "IndexExpr",
        Expr: expr.(ast.Expr),
        Index: index.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / expr:DerefExpr "." id:DerefKey {
      return &ast.BinaryExpr{
        Kind: "BinaryExpr",
        Op: ".",
        LHS: expr.(ast.Expr),
        RHS: id.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
  / FuncExpr
  / Primary

DerefKey
  = Identifier 
  / s:DoubleQuotedString    { return &ast.ID{Kind: "ID", Name:s.(string), Loc: loc(c)}, nil }
  / s:BacktickString        { return &ast.ID{Kind: "ID", Name:s.(string), Loc: loc(c)}, nil }

FuncExpr
  = Cast
  / Function

FuncGuard = NotFuncs __ "("

NotFuncs
  = NOT
  / SELECT

Cast
  = typ:TypeLiteral __ "(" __ expr:(OverExpr / Expr) __ ")" {
      return &ast.Cast{Kind: "Cast", Expr: expr.(ast.Expr), Type: typ.(ast.Expr), Loc: loc(c)}, nil
    }

Function
  = Grep
  // Special case to handle `regexp(/x/, y)` and `regexp_replace(x, /y/, z)`.
  / REGEXP __ "(" __ arg0:RegexpPrimitive __ "," __ arg1:Expr __ ")" where:WhereClause? {
      id := &ast.ID{Kind: "ID", Name: "regexp", Loc: ast.NewLoc(c.pos.offset, c.pos.offset+6)}
      return newCall(c, id, []any{arg0, arg1}, where), nil
    }
  / REGEXP_REPLACE __ "(" __ arg0:Expr __ "," __ arg1:RegexpPrimitive __ "," __ arg2:Expr __ ")" where:WhereClause? {
      id := &ast.ID{Kind: "ID", Name: "regexp_replace", Loc: ast.NewLoc(c.pos.offset, c.pos.offset+14)}
      return newCall(c, id, []any{arg0, arg1, arg2}, where), nil
    }
  / !FuncGuard EXTRACT __ "(" __ part:Expr _ FROM _ e:Expr __ ")" where:WhereClause? {
      return &ast.CallExtract{
        Kind: "CallExtract",
        Part: part.(ast.Expr),
        Expr: e.(ast.Expr), 
        Loc: loc(c),
      }, nil
    }
  / !FuncGuard CAST __ "(" __ e:Expr _ AS _ typ:Identifier __ ")" {
      return &ast.SQLCast{
        Kind: "SQLCast",
        Expr: e.(ast.Expr),
        Type: typ.(*ast.ID),
        Loc: loc(c),
      }, nil
    }
  / !FuncGuard SUBSTRING __ "(" __ expr:Expr from:(_ FROM _ e:Expr { return e, nil })? for_:(_ FOR _ e:Expr { return e, nil })? ")" {
      s := &ast.SQLSubstring{
        Kind: "SQLSubstring",
        Expr: expr.(ast.Expr),
        Loc: loc(c),
      }
      if from != nil {
        s.From = from.(ast.Expr)
      }
      if for_ != nil {
        s.For = for_.(ast.Expr)
      }
      return s, nil
    }
  / !FuncGuard fn:Identifier __ "(" __ args:FunctionArgs __ ")" where:WhereClause? {
      return newCall(c, fn, args, where), nil
    }
  / CountStar

RegexpPrimitive
  = pat:RegexpPattern { return newPrimitive(c, "string", pat.(string)), nil }

FunctionArgs
  = o:OverExpr { return []any{o}, nil }
  / OptionalExprs

Grep
  = GREP __ "(" __ pattern:(Regexp / Glob / Expr) __ opt:("," __ e:(OverExpr / Expr) __ { return e, nil })? ")" {
      g := &ast.Grep{
          Kind: "Grep",
          Pattern: pattern.(ast.Expr),
          Loc: loc(c),
      }
      if opt != nil {
          g.Expr = opt.(ast.Expr)
      }
      return g, nil
    }

OptionalExprs
  = Exprs
  / __ { return []any{}, nil }

Exprs
  = first:Expr rest:(__ "," __ e:Expr { return e, nil })* {
      return prepend(first, rest), nil
    }

Primary
  = CaseExpr
  / Record
  / Array
  / Set
  / Map
  / SQLTimeValue
  / Literal
  / !PipeKeyword id:Identifier { return id, nil }
  / Tuple
  / "(" __ expr:OverExpr __ ")" { return expr, nil }
  / "(" __ expr:Expr __ ")" { return expr, nil }

CaseExpr
  = CASE cases:When+ else_:(_ ELSE _ Expr)? _ END (_ CASE)? {
        whens := sliceOf[ast.When](cases)
        cond := &ast.Conditional{
            Kind: "Conditional",
            Cond: whens[0].Cond,
            Then: whens[0].Then,
            Loc: loc(c),
        }
        last := cond
        for _, when := range whens[1:] {
            next := &ast.Conditional{
               Kind: "Conditional",
               Cond: when.Cond,
               Then: when.Then,
               Loc: when.Loc,
            }
            last.Else = next
            last = next
        }
        if else_ != nil {
            last.Else = else_.([]any)[3].(ast.Expr)
        }
        return cond, nil
    }
  / CASE _ expr:Expr whens:When+ else_:(_ ELSE _ Expr)? _ END (_ CASE)?  {
        cond := &ast.CaseExpr{
            Kind: "CaseExpr",
            Expr: expr.(ast.Expr),
            Whens: sliceOf[ast.When](whens),
            Loc: loc(c),
        }
        if else_ != nil {
            cond.Else = else_.([]any)[3].(ast.Expr)
        }
        return cond, nil
    }

When
  = _ WHEN _ cond:Expr _ THEN _ then:Expr {
      return ast.When{
        Kind: "When",
        Cond: cond.(ast.Expr),
        Then: then.(ast.Expr),
        Loc: loc(c),
      }, nil
    }
    
OverExpr
  = OVER _ exprs:Exprs locals:Locals? __ Pipe __ body:Seq {
      return &ast.OverExpr{
          Kind: "OverExpr",
          Locals: sliceOf[ast.Def](locals),
          Exprs: sliceOf[ast.Expr](exprs),
          Body: sliceOf[ast.Op](body),
          Loc: loc(c),
      }, nil
    }

Record
  = "{" __ elems:RecordElems __ "}" {
      return &ast.RecordExpr{
          Kind: "RecordExpr",
          Elems: sliceOf[ast.RecordElem](elems),
          Loc: loc(c),
      }, nil
    }

RecordElems
  = first:RecordElem rest:RecordElemTail* {
    return prepend(first, rest), nil
  }
  / __ { return nil, nil }

RecordElemTail = __ "," __ elem:RecordElem { return elem, nil }

RecordElem
  = Spread
  / FieldExpr
  / Identifier

Spread
  = "..." __ expr:Expr {
      return &ast.Spread{Kind: "Spread", Expr: expr.(ast.Expr), Loc: loc(c)}, nil
    }

FieldExpr
  = name:Name __ ":" __ value:Expr {
      return &ast.FieldExpr{
        Kind: "FieldExpr",
        Name: name.(*ast.Name),
        Value: value.(ast.Expr),
        Loc: loc(c),
      }, nil
    }

Array
  = "[" __ elems:VectorElems __ "]" {
      return &ast.ArrayExpr{
          Kind: "ArrayExpr",
          Elems: sliceOf[ast.VectorElem](elems),
          Loc: loc(c),
      }, nil
    }

Set
  = "|[" __ elems:VectorElems __ "]|" {
      return &ast.SetExpr{
          Kind: "SetExpr",
          Elems: sliceOf[ast.VectorElem](elems),
          Loc: loc(c),
      }, nil
    }

VectorElems
  = first:VectorElem rest:(__ "," __ e:VectorElem { return e, nil })* {
      return prepend(first, rest), nil
    }
  / __ { return nil, nil }

VectorElem
  = Spread
  / e:Expr { return &ast.VectorValue{Kind: "VectorValue", Expr: e.(ast.Expr),Loc:loc(c)}, nil }

Map
  = "|{" __ exprs:Entries __ "}|" {
      return &ast.MapExpr{
          Kind: "MapExpr",
          Entries: sliceOf[ast.EntryExpr](exprs),
          Loc: loc(c),
      }, nil
    }

Entries
  = first:Entry rest:EntryTail* {
    return prepend(first, rest), nil
  }
  / __ { return nil, nil }


EntryTail = __ "," __ e:Entry { return e, nil }

Entry
  = key:Expr __ ":" __ value:Expr {
      return ast.EntryExpr{Key: key.(ast.Expr), Value: value.(ast.Expr),Loc:loc(c)}, nil
    }

Tuple
  = "(" __ first:Expr rest:(__ "," __ e:Expr { return e, nil })+ __ ")" {
      return &ast.TupleExpr{
        Kind: "TupleExpr",
        Elems: sliceOf[ast.Expr](prepend(first, rest)),
        Loc: loc(c),
      }, nil
    }

SQLTimeValue
  = typ:(DATE / TIMESTAMP) _ s:StringLiteral {
        return &ast.SQLTimeValue {
          Kind: "SQLTimeValue",
          Type: typ.(string),
          Value: s.(*ast.Primitive),
          Loc: loc(c),
        }, nil
    }

/// === Literals ===

Literal
  = TypeLiteral
  / StringLiteral
  / FString
  / SubnetLiteral
  / AddressLiteral
  / BytesLiteral
  / Duration
  / Time
  / FloatLiteral
  / IntegerLiteral
  / BooleanLiteral
  / NullLiteral

SubnetLiteral
  = v:IP6Net !IdentifierRest {
      return newPrimitive(c, "net", v.(string)), nil
    }
  / v:IP4Net {
      return newPrimitive(c, "net", v.(string)), nil
    }

AddressLiteral
  = v:IP6 !IdentifierRest {
      return newPrimitive(c, "ip", v.(string)), nil
    }
  / v:IP {
      return newPrimitive(c, "ip", v.(string)), nil
    }

FloatLiteral
  = v:FloatString {
      return newPrimitive(c, "float64", v.(string)), nil
    }

IntegerLiteral
  = v:IntString {
      return newPrimitive(c, "int64", v.(string)), nil
    }

BooleanLiteral
  = TRUE  { return newPrimitive(c, "bool", "true"), nil }
  / FALSE { return newPrimitive(c, "bool", "false"), nil }

NullLiteral
  = NULL { return newPrimitive(c, "null", ""), nil }

BytesLiteral
  = "0x" HexDigit* {
    return newPrimitive(c, "bytes", string(c.text)), nil
  }

TypeLiteral
  = "<" typ:Type ">" {
      return &ast.TypeValue{
        Kind:"TypeValue",
        Value: typ.(ast.Type),
        Loc: loc(c),
      }, nil
    }

Type
  = AmbiguousType
  / ComplexType

AmbiguousType
  = name:PrimitiveType !IdentifierRest { return name, nil }
  / ERROR __ "(" __ t:Type __ ")" {
      return &ast.TypeError{
          Kind: "TypeError",
          Type: t.(ast.Type),
          Loc: loc(c),
      }, nil
    }
  / name:Name opt:(__ '=' __ Type)? {
      if opt != nil {
        return &ast.TypeDef{
            Kind: "TypeDef",
            Name: name.(*ast.Name).Text,
            Type: opt.([]any)[3].(ast.Type),
            Loc: loc(c),
        }, nil
      }
      return &ast.TypeName{Kind: "TypeName", Name: name.(*ast.Name).Text, Loc: loc(c)}, nil
    }
  / "(" __ types:TypeList ")" {
      return &ast.TypeUnion{
        Kind: "TypeUnion",
        Types: sliceOf[ast.Type](types),
        Loc: loc(c),
      }, nil
    }

TypeList
  = first:Type rest:TypeListTail+ {
    return prepend(first, rest), nil
  }

TypeListTail = __ "," __ typ:Type { return typ, nil }

ComplexType
  = "{" __ fields:TypeFieldList __ "}" {
      return &ast.TypeRecord{
          Kind: "TypeRecord",
          Fields: sliceOf[ast.TypeField](fields),
          Loc: loc(c),
      }, nil
    }
  / "[" __ typ:Type __ "]" {
      return &ast.TypeArray{
          Kind: "TypeArray",
          Type: typ.(ast.Type),
          Loc: loc(c),
      }, nil
    }
  / "|[" __ typ:Type __ "]|" {
      return &ast.TypeSet{
          Kind: "TypeSet",
          Type: typ.(ast.Type),
          Loc: loc(c),
      }, nil
    }
  / "|{" __ keyType:Type __ ":" __ valType:Type __ "}|" {
      return &ast.TypeMap{
        Kind: "TypeMap",
        KeyType: keyType.(ast.Type),
        ValType: valType.(ast.Type),
        Loc: loc(c),
      }, nil
    }

StringLiteral
  = s:DoubleQuotedString { return &ast.DoubleQuote{Kind:"DoubleQuote", Text: s.(string), Loc: loc(c)}, nil }
  / s:SingleQuotedString { return newPrimitive(c, "string", s.(string)), nil }

FString
  = "f\"" v:FStringDoubleQuotedElem* '"' {
      return &ast.FString{
        Kind: "FString",
        Elems: sliceOf[ast.FStringElem](v),
        Loc: loc(c),
      }, nil
    }
  / "f'" v:FStringSingleQuotedElem* "'" {
      return &ast.FString{
        Kind: "FString",
        Elems: sliceOf[ast.FStringElem](v),
        Loc: loc(c),
      }, nil
    }

FStringDoubleQuotedElem
  = FStringExpr
  / v:FStringDoubleQuotedChar+ {
      return &ast.FStringText{Kind: "FStringText", Loc: loc(c), Text: joinChars(v)}, nil
    }

FStringDoubleQuotedChar
  = "\\" v:"{" { return v, nil }
  / !("{") v:DoubleQuotedChar { return v, nil }

FStringSingleQuotedElem
  = FStringExpr
  / v:FStringSingleQuotedChar+ {
      return &ast.FStringText{Kind: "FStringText", Loc: loc(c), Text: joinChars(v)}, nil
    }

FStringSingleQuotedChar
  = "\\" v:"{" { return v, nil }
  / !("{") v:SingleQuotedChar { return v, nil }

FStringExpr
  = "{" __ e:Expr __ "}" {
      return &ast.FStringExpr{
        Kind: "FStringExpr",
        Expr: e.(ast.Expr),
        Loc: loc(c),
      }, nil
    }

PrimitiveType
  = (   "uint8" / "uint16" / "uint32" / "uint64"
      / "int8" / "int16" / "int32" / "int64"
      / "float16" / "float32" / "float64"
      / "bool" / "string"
      / "duration" / "time"
      / "bytes"
      / "ip" / "net"
      / "type" / "null") {
            return &ast.TypePrimitive{
                Kind: "TypePrimitive",
                Name: string(c.text),
                Loc: loc(c),
            }, nil
        }

TypeFieldList
  = first:TypeField rest:TypeFieldListTail* {
    return prepend(first, rest), nil
  }
  / "" { return nil, nil }

TypeFieldListTail = __ "," __ typ:TypeField { return typ, nil }

TypeField
  = name:Name __ ":" __ typ:Type {
      return ast.TypeField{
          Name: name.(*ast.Name).Text,
          Type: typ.(ast.Type),
          Loc: loc(c),
      }, nil
    }

Name
  = s:DottedIDs          { return &ast.Name{Kind:"Name",Text:s.(string),Loc:loc(c) }, nil }
  / s:IdentifierName     { return &ast.Name{Kind:"Name",Text:s.(string),Loc:loc(c) }, nil }
  / s:DoubleQuotedString { return &ast.Name{Kind:"Name",Text:s.(string),Loc:loc(c) }, nil }
  / s:SingleQuotedString { return &ast.Name{Kind:"Name",Text:s.(string),Loc:loc(c) }, nil }
  / s:KSUID              { return &ast.Name{Kind:"Name",Text:s.(string),Loc:loc(c) }, nil }

DottedIDs
  = (IdentifierStart / ".") (IdentifierRest / ".")* { return string(c.text), nil }

Identifier
  = id:IdentifierName {
      return &ast.ID{
          Kind: "ID",
          Name: id.(string),
          Loc: loc(c),
      }, nil
    }

Identifiers
  = first:Identifier rest:(__ "," __ name:Identifier { return name, nil })* {
    return prepend(first, rest), nil
  }

SQLIdentifier
  = Identifier
  / s:BacktickString     { return &ast.ID{Kind:"ID", Name:s.(string), Loc:loc(c) }, nil }
  / s:DoubleQuotedString { return &ast.ID{Kind:"ID", Name:s.(string), Loc:loc(c) }, nil }

IdentifierName
  = !(IDGuard !IdentifierRest) IdentifierStart IdentifierRest* { return string(c.text), nil }
  / "$" { return string(c.text), nil }
  / "\\" id:IDGuard { return id, nil }
  // "type" is a search guard but should not be an id guard
  / "type" { return string(c.text), nil }
  / BacktickString 

IdentifierStart
  = UnicodeLetter
  / "$"
  / "_"

IdentifierRest
  = IdentifierStart
  / UnicodeCombiningMark
  / UnicodeDigit
  / UnicodeConnectorPunctuation

IDGuard
  = BooleanLiteral
  / NullLiteral
  / NaN
  / Infinity

Time
  = FullDate "T" FullTime {
      return newPrimitive(c, "time", string(c.text)), nil
    }

FullDate = D4 "-" D2 "-" D2

D4 = [0-9][0-9][0-9][0-9]
D2 = [0-9][0-9]

FullTime = PartialTime TimeOffset

PartialTime = D2 ":" D2 ":" D2 ("." [0-9]+)?

TimeOffset
  = "Z"
  / ("+" / "-") D2 ":" D2 ("." [0-9]+)?

Duration
  = "-"? (Decimal TimeUnit)+ {
      return newPrimitive(c, "duration", string(c.text)), nil
    }

Decimal = UInt ("." UInt)?

TimeUnit
  = "ns"
  / "us"
  / "ms"
  / "s"
  / "m"
  / "h"
  / "d"
  / "w"
  / "y"

IP
  = UInt "." UInt "." UInt "." UInt { return string(c.text), nil }

IP6
  = !(Hex ":" Hex !(HexDigit / ":")) v:IP6Variations { return v, nil }

// this matches a superset of legal syntax for ip6 addresses but the compiler
// will catch any errors when translating the filter
IP6Variations
  = a:HexColon+ b:IP6Tail {
      return joinChars(a) + b.(string), nil
    }
  / a:Hex b:ColonHex* "::" d:HexColon* e:IP6Tail {
      return a.(string) + joinChars(b) + "::" + joinChars(d) + e.(string), nil
    }
  / "::" a:HexColon* b:IP6Tail {
      return "::" + joinChars(a) + b.(string), nil
    }
  / a:Hex b:ColonHex* "::" {
      return a.(string) + joinChars(b) + "::", nil
    }
  / "::" {
      return "::", nil
    }

IP6Tail
  = IP
  / Hex

ColonHex = ":" v:Hex { return ":" + v.(string), nil }

HexColon = v:Hex ":" { return v.(string) + ":", nil }

IP4Net
  = a:IP '/' m:UIntString {
      return a.(string) + "/" + m.(string), nil
    }

IP6Net
  = a:IP6 '/' m:UIntString {
      return a.(string) + "/" + m.(string), nil
    }

UInt
 = s:UIntString { return parseInt(s), nil }

IntString
  = UIntString
  / MinusIntString

UIntString = [0-9]+ { return string(c.text), nil }

MinusIntString
  = "-" UIntString { return string(c.text), nil }

FloatString
  = "-"? [0-9]+ "." [0-9]* ExponentPart? { return string(c.text), nil }
  / "-"? "." [0-9]+ ExponentPart? { return string(c.text), nil }
  / (NaN / Infinity) { return string(c.text), nil }


ExponentPart = "e"i [+-]? UIntString

NaN = "NaN"

Infinity = ("-" / "+")? "Inf"

Hex = HexDigit+ { return string(c.text), nil }

HexDigit = [0-9a-fA-F]

SingleQuotedString
  = "'" v:SingleQuotedChar* "'" { return joinChars(v), nil }

DoubleQuotedString
  = '"' v:DoubleQuotedChar* '"' { return joinChars(v), nil }

DoubleQuotedChar
  = !('"' / EscapedChar) . { return string(c.text), nil }
  / "\\" s:EscapeSequence { return s, nil }

BacktickString
  = '`' v:BacktickChar* '`' { return joinChars(v), nil }

BacktickChar
  = !('`' / EscapedChar) . { return string(c.text), nil }
  / "\\" s:EscapeSequence { return s, nil }

KeyWord
  = head:KeyWordStart tail:KeyWordRest* { return head.(string) + joinChars(tail), nil }

KeyWordStart
  = KeyWordChars
  / KeyWordEsc

KeyWordRest
  = KeyWordStart
  / [0-9]

KeyWordChars = (UnicodeLetter / [_.:/%#@~]) { return string(c.text), nil }

KeyWordEsc = "\\" s:(KeywordEscape / EscapeSequence) { return s, nil }

GlobPattern
