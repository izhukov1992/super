// XXX We will leave multi-line comments out for now since there is some work
// that needs to be done disambiguating among KeyWord, Regexp, and "/*".
// We will tackle this in the search-expr branch that is changing the grammar
// to better mix together ad hoc keyword search with formal boolean expressions.
Comment "comment"
  // = MultiLineComment
  // / SingleLineComment
  = SingleLineComment

MultiLineComment
  = "/*" (!"*/" SourceCharacter)* "*/"

SingleLineComment
  = "//" (!LineTerminator SourceCharacter)*
  / "--" (!LineTerminator SourceCharacter)*

EOL = WhiteSpace* LineTerminator

EOT = _ / EOF

EOF = !.

EOKW = !KeyWordChars

/// === SuperSQL ===

SQLPipe 
  = s:Seq {
      return &ast.SQLPipe{
            Kind: "SQLPipe",
            Ops: sliceOf[ast.Op](s),
            Loc: loc(c),
        }, nil
    }

SelectOp = !"(" op:SelectExpr { return op, nil}

SelectExpr 
  = with:OptWithClause
    body:(
        SetOperation
      / Select
      / FromSelect
      / Values
      / "(" __ s:SQLPipe __")"      { return s, nil }
    )
    orderby:OptOrderByClause
    loff:OptSQLLimitOffset {
      op := body.(ast.Op)
      if with != nil {
          w := with.(*ast.With)
          w.Body = op
          op = w 
      }
      if orderby != nil {
          o := orderby.(*ast.OrderBy)
          o.Op = op
          op = o
      }
      if loff != nil {
          o := loff.(*ast.SQLLimitOffset)
          o.Op = op
          op = o
      }
      return op, nil
    }

Select 
  = SELECT 
    distinct:OptDistinct
    value:OptSelectValue
    //XXX for whitespace in front of target need it only if it begins with 
    // an identifier, e.g., select(1); is ok
    _ selection:Selection
    from:OptFromClause
    where:OptWhereClause
    group:OptGroupClause
    having:OptHavingClause
    // XXX NOTYET OptQualifyClause
    // XXX NOTYET OptWindowClause
    {
      sel := &ast.Select{
          Kind: "Select",
          Distinct: distinct.(bool),
          Value: value.(bool),
          Selection: selection.(ast.Selection),
          Loc: loc(c),
      }
      if from != nil {
        sel.From = from.(*ast.From)
      }
      if where != nil {
        sel.Where = where.(ast.Expr)
      }
      if group != nil {
        sel.GroupBy = sliceOf[ast.Expr](group)
      }
      if having != nil {
        sel.Having = having.(ast.Expr)
      }
      return sel, nil
    }

FromSelect
  = from:FromOp _ SELECT 
    distinct:OptDistinct
    value:OptSelectValue
    //XXX for whitespace in front of target need it only if it begins with 
    // an identifier, e.g., select(1); is ok
    _ selection:Selection
    where:OptWhereClause
    group:OptGroupClause
    having:OptHavingClause
    // XXX NOTYET OptQualifyClause
    // XXX NOTYET OptWindowClause
    {
      sel := &ast.Select{
          Kind: "Select",
          Distinct: distinct.(bool),
          Value: value.(bool),
          Selection: selection.(ast.Selection),
          From: from.(*ast.From),
          Loc: loc(c),
      }
      if where != nil {
        sel.Where = where.(ast.Expr)
      }
      if group != nil {
        sel.GroupBy = sliceOf[ast.Expr](group)
      }
      if having != nil {
        sel.Having = having.(ast.Expr)
      }
      return sel, nil
    }

Values
  = VALUES _ exprs:Exprs {
      return &ast.Values{
          Kind: "Values",
          Exprs: sliceOf[ast.Expr](exprs),
          Loc: loc(c),
      }, nil
    }

OptDistinct
  = _ ALL        { return false, nil }
  / _ DISTINCT   { return true, nil }
  / ""              { return false, nil }

OptSelectValue
  = _ AS _ VALUE  { return true, nil }
  / _ VALUE          { return true, nil } // SQL++ form XXX decide which
  / ""                  { return false, nil }

OptWithClause 
  = WithClause
  / ""          { return nil, nil }

WithClause 
  = WITH r:OptRecursive _ ctes:CteList __ {
        return &ast.With{
            Kind: "With",
            Recursive: r.(bool), 
            CTEs: sliceOf[ast.CTE](ctes),
            Loc: loc(c),
        }, nil
    }

OptRecursive 
  = _ RECURSIVE  { return true, nil }
  / ""              { return false, nil }

CteList = first:Cte rest:( __ "," __ cte:Cte { return cte, nil} )* {
    return prepend(first, rest), nil 
}

Cte
  = name:SQLIdentifier _ AS m:OptMaterialized __ "(" __ s:SQLPipe __ ")" {
        return ast.CTE{
            Name: name.(*ast.ID),
            Materialized: m.(bool),
            Body: s.(*ast.SQLPipe),
            Loc: loc(c),
        }, nil
    }

OptMaterialized 
  = _ MATERIALIZED _             { return true, nil }
  / _ NOT _ MATERIALIZED _    { return false, nil }
  / ""                              { return false, nil }

OptAllClause
  = _ ALL 
  / ""

OptFromClause
  = _ from:FromOp {
      return from, nil
    }
  / ""                  { return nil, nil }

OptWhereClause
  = where:WhereClause     { return where, nil }
  / ""                      { return nil, nil }

OptGroupClause
  = _ group:GroupClause { return group, nil }
  / ""                  { return nil, nil }

GroupClause
  = GROUP _ BY _ list:GroupByList { return list, nil }

GroupByList 
  = first:GroupByItem rest:( __ "," __ g:GroupByItem { return g, nil } )* {
      return prepend(first, rest), nil
    }

GroupByItem = Expr

OptHavingClause
  = _ h:HavingClause { return h, nil }
  / "" { return nil, nil }

HavingClause
  = HAVING _ e:Expr { return e, nil }

JoinOperation
  = CrossJoin
  / ConditionJoin

CrossJoin
  = left:FromElem CrossJoinOp right:FromElem {
        return &ast.CrossJoin{
            Kind: "CrossJoin",
            Left: left.(*ast.FromElem),
            Right: right.(*ast.FromElem),
            Loc: loc(c),
        }, nil
    }

CrossJoinOp 
  = __ "," __ 
  / _ CROSS _ JOIN _ 

ConditionJoin
  = left:FromElem style:SQLJoinStyle _ right:FromElem e:JoinExpr {
        return &ast.SQLJoin{
            Kind: "SQLJoin",
            Style: style.(string),
            Left: left.(*ast.FromElem),
            Right: right.(*ast.FromElem),
            Cond: e.(ast.JoinExpr),
            Loc: loc(c),
        }, nil
    }

SQLJoinStyle 
  = (_ INNER)? _ JOIN             { return "inner", nil }
  / _ FULL (_ OUTER)? _ JOIN   { return "full", nil }
  / _ LEFT (_ OUTER)? _ JOIN   { return "left", nil }
  / _ RIGHT (_ OUTER)? _ JOIN   { return "right", nil }

JoinExpr 
  = _ ON _ e:Expr { 
        return &ast.JoinOnExpr{
            Kind: "JoinOnExpr", 
            Expr: e.(ast.Expr),
            Loc: loc(c),
        }, nil
    }
  / _ USING __ "(" __ fields:Lvals __ ")" {
        return &ast.JoinUsingExpr{
            Kind: "JoinUsingExpr",
            Fields: sliceOf[ast.Expr](fields),
            Loc: loc(c),
        }, nil
    }

OptOrdinality 
  = _ WITH _ ORDINALITY   {
        return &ast.Ordinality{
            Kind: "Ordinality",
            Loc: loc(c),
        }, nil
    }
  / ""                          { return nil, nil }

OptAlias
  = _ a:AliasClause { return a, nil }
  / ""              { return nil, nil }

AliasClause
 = (AS _)? !(SQLGuard / DeprecatedFroms) name:IdentifierName cols:Columns? {
      alias := &ast.TableAlias{Kind: "TableAlias", Name: name.(string), Loc: loc(c)}
      if cols != nil {
        alias.Columns = cols.([]*ast.ID)
      }
      return alias, nil
    }

Columns
  = __ "(" __ first:SQLIdentifier rest:( __ "," __ s:SQLIdentifier{ return s, nil })* __ ")" {
        return sliceOf[*ast.ID](prepend(first,rest)), nil 
    }

Selection
  = first:SelectElem rest:( __ "," __ s:SelectElem{ return s, nil })* {
      return ast.Selection{
        Kind: "Selection",
        Args: sliceOf[ast.AsExpr](prepend(first, rest)),
        Loc: loc(c),
      }, nil
  }

//XXX need to add *, id.*, except, replace
SelectElem 
  = expr:(AggDistinct / Expr) as:OptAsClause {
      elem := ast.AsExpr{
        Kind: "AsExpr",
        Expr: expr.(ast.Expr),
        Loc: loc(c),
      }
      if as != nil {
          elem.Label = as.(*ast.ID)
      }
      return elem, nil
    }
  / "*" {
      // empty assignment is star (for now)
      return ast.AsExpr{Kind: "AsExpr"}, nil
    }

OptAsClause 
  = _ AS _ id:SQLIdentifier { return id, nil }
  / _ !SQLGuard id:SQLIdentifier { return id, nil }
  / ""  { return nil, nil }

OptOrderByClause 
  = _ ORDER _ BY _ list:OrderByList { 
        return &ast.OrderBy{
          Kind: "OrderBy",
          Exprs: list.([]ast.SortExpr),
          Loc: loc(c),
        }, nil
    }
  / ""          { return nil, nil }

OrderByList 
  = first:OrderByItem rest:( __ "," __ o:OrderByItem{ return o, nil})* {
      return sliceOf[ast.SortExpr](prepend(first, rest)), nil
    }

OrderByItem
  = e:Expr order:OptAscDesc nulls:OptNullsOrder {
      s := ast.SortExpr{Kind: "SortExpr", Expr: e.(ast.Expr), Loc: loc(c)}
      if order != nil {
        s.Order = order.(*ast.ID)
      }
      if nulls != nil {
        s.Nulls = nulls.(*ast.ID)
      }
      return s, nil
    }

OptAscDesc
  = _ ASC   { return &ast.ID{Kind: "ID", Name: "asc", Loc: loc(c)}, nil }
  / _ DESC { return &ast.ID{Kind: "ID", Name: "desc", Loc: loc(c)}, nil }
  / ""         { return nil, nil }

OptNullsOrder
  = _ NULLS _ FIRST   { return &ast.ID{Kind: "ID", Name: "first", Loc: loc(c)}, nil }
  / _ NULLS _ LAST    { return &ast.ID{Kind: "ID", Name: "last", Loc: loc(c)}, nil }
  / ""                      { return nil, nil }

OptSQLLimitOffset
  = _ op:SQLLimitOffset { return op, nil } 
  / ""                  { return nil, nil }

SQLLimitOffset
  = l:LimitClause o:OptOffsetClause {
      op := &ast.SQLLimitOffset{
        Kind: "SQLLimitOffset",
        Loc: loc(c),
      }
      if l != nil {
        op.Limit = l.(ast.Expr)
      }
      if o != nil {
        op.Offset = o.(ast.Expr)
      }
      return op, nil
    }
  / o:OffsetClause l:OptLimitClause {
      op := &ast.SQLLimitOffset{
        Kind: "SQLLimitOffset",
        Offset: o.(ast.Expr),
        Loc: loc(c),
      }
      if l != nil {
        op.Limit = l.(ast.Expr)
      }
      return op, nil
    }

OptLimitClause 
  = _ l:LimitClause { return l, nil }
  / ""              { return nil, nil }

LimitClause 
  = LIMIT _ ALL { return nil, nil }
  / LIMIT _ e:Expr { return e, nil }

OptOffsetClause 
  = _ o:OffsetClause  { return o, nil }
  / ""  { return nil, nil }

OffsetClause 
  = OFFSET _ e:Expr  { return e, nil }

SetOperation
  = left:SelectExpr distinct:SetOp _ right:SelectExpr {
        return &ast.Union{
            Kind: "Union",
            Distinct: distinct.(bool),
            Left: left.(ast.Op),
            Right: right.(ast.Op),
            Loc: loc(c),
        }, nil
    }

SetOp 
  = _ UNION _ ALL       { return false, nil }
  / _ UNION (_ DISTINCT)?  { return true, nil }

